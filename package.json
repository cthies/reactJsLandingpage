    "lodash.throttle": "^4.1.1",
    "lodash.uniqueid": "^4.0.1",
    "next": "^11.1.0",
    "next-api-url": "^1.0.0",
    "pino-http": "^5.8.0",
    "prismic-dom": "^2.2.6",
    "react": "^17.0.2",
    "react-cool-dimensions": "^2.0.7",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-popper-tooltip": "^4.3.0",
    "react-redux": "^7.2.4",
    "react-slick": "^0.28.1",
    "react-youtube": "^7.13.1",
    "sharp": "^0.30.1",
    "slick-carousel": "^1.8.1"
  },
